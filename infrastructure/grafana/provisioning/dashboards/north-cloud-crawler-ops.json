      "datasource": { "type": "elasticsearch", "uid": "elasticsearch" },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "elasticsearch", "uid": "elasticsearch" },
          "query": "*",
          "alias": "",
          "metrics": [
            {
              "type": "count",
              "id": "1"
            }
          ],
          "bucketAggs": [
            {
              "type": "terms",
              "field": "_index",
              "id": "2",
              "settings": {
                "size": "15",
                "order": "desc",
                "orderBy": "1",
                "min_doc_count": "1"
              }
            }
          ],
          "timeField": "crawled_at"
