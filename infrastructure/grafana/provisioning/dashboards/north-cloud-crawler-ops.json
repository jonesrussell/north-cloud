    {
      "id": 14,
      "title": "Job Completion Log",
      "description": "Recent crawl job completions with items crawled/indexed and duration",
      "type": "table",
      "datasource": { "type": "loki", "uid": "loki" },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 26 },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "loki", "uid": "loki" },
          "expr": "{service=\"crawler\"} |= \"Job completed successfully\" | json",
          "queryType": "range"
        }
      ],
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "Line",
            "format": "json"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "id": true,
              "tsNs": true,
              "Line": true,
              "labels": true,
              "caller": true,
              "level": true,
              "service": true,
              "msg": true
            },
            "renameByName": {
              "Time": "Completed At",
              "job_id": "Job ID",
              "duration_ms": "Duration (ms)",
              "items_crawled": "Pages Crawled",
              "items_indexed": "Items Indexed",
              "next_run_at": "Next Run"
            },
            "indexByName": {
              "Time": 0,
              "job_id": 1,
              "items_crawled": 2,
              "items_indexed": 3,
              "duration_ms": 4,
              "next_run_at": 5
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Duration (ms)" },
            "properties": [
              { "id": "unit", "value": "ms" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Completed At", "desc": true }]
      }
    },
    {
      "id": 15,
      "type": "row",
      "title": "Crawler Errors",
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "panels": [
        {
          "id": 16,
          "title": "Error Stream",
          "description": "Error-level logs from the crawler service",
          "type": "logs",
          "datasource": { "type": "loki", "uid": "loki" },
          "gridPos": { "h": 12, "w": 24, "x": 0, "y": 37 },
          "targets": [
            {
              "refId": "A",
              "datasource": { "type": "loki", "uid": "loki" },
              "expr": "{service=\"crawler\", level=~\"error|warn\"} | json | line_format \"[{{.level}}] {{.msg}} {{if .error}}err={{.error}}{{end}} {{if .url}}url={{.url}}{{end}}\"",
              "queryType": "range"
            }
          ],
          "options": {
            "showTime": true,
            "showLabels": false,
            "showCommonLabels": false,
            "wrapLogMessage": true,
            "prettifyLogMessage": true,
            "enableLogDetails": true,
            "sortOrder": "Descending",
            "dedupStrategy": "signature"
          }
        }
      ]
    }
